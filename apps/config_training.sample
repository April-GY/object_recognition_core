# info about the db
db:
  type: 'CouchDB'
  root: 'http://localhost:5984'
  collection: 'object_recognition'

pipeline1:
  type: 'TOD'
  feature_descriptor:
    combination: 'ORB'
    feature:
      n_features: 1000
  search:
    key_size: 18
    multi_probe_level: 2
    n_tables: 16
    radius: 50
    ratio: 0.8
    type: 'LSH'

# The list of object_ids to analyze
object_ids: "all"
